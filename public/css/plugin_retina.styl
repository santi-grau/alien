@import 'nib'

// SMALL ALIEN
smallStepsFire = 4

.smallAlien
  width 50px
  height 32px
  position absolute
  animation smallWalkFire smallStepsFire * 0.8s steps(smallStepsFire) forwards, smallFire 0s steps(1) forwards
  animation-delay 0s, smallStepsFire * 0.8s
  top 100px
  margin-left -29px
  .inner
    position absolute
    width 100%
    height 100%
    background-image url("data:image/gif;base64,")
    background-size auto 100%
    animation smallWalkInner .8s steps(12) smallStepsFire forwards, smallFireInner 1s steps(6) 1 forwards
    animation-delay 0s, smallStepsFire * 0.8s

@keyframes smallWalkFire
  0%
    left 0px
  100%
    left 24px * smallStepsFire

@keyframes smallFire
  0%
    width 50px
  100%
    width 32px

@keyframes smallWalkInner
  0%
    background-position 0px 0px
  100%
    background-position -600px 0

@keyframes smallFireInner
  0%
    background-position -600px 0px
  100%
    background-position -792px 0